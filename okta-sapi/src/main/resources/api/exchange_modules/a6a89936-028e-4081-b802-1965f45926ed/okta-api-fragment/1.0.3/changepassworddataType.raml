#%RAML 1.0 DataType
type: object
displayName: Change Password
properties:
  oldPassword:
    type: object
    required: true
    displayName: "oldPassword"
    properties:
      value:
        type: string
        displayName: "value"
        example: "follow@432"
  newPassword:
    type: object
    required: true
    displayName: "newPassword"   
    properties:
      value:
        type: string
        displayName: "value" 
        example: "setitup#12"
          
      